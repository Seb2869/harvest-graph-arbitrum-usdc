{
  "name": "arbitrum-plasma",
  "license": "UNLICENSED",
  "scripts": {
    "deploy-studio": "graph codegen && graph build && graph deploy -l v0.0.11 harvest-plasma-usdc-arbitrum && npm run after-deploy",
    "deploy-studio-test": "graph codegen && graph build && graph deploy -l 0.0.16 harvest-plasma-usdc-arbitrum-test",
    "codegen": "graph codegen",
